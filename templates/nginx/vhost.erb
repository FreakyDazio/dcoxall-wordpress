# Redirect everything to the main site. We use a separate server statement and NOT an if statement - see http://wiki.nginx.org/IfIsEvil
server {
  server_name _;
  root /vagrant;

  include /etc/nginx/wordpress.d/restrictions.conf;

  # Additional rules go here.

  # Only include one of the files below.
  include /etc/nginx/wordpress.d/wordpress.conf;
# include /etc/nginx/wordpress.d/wordpress-ms-subdir.conf;
# include /etc/nginx/wordpress.d/wordpress-ms-subdomain.conf;
}
